    <name value="CHORFServiceRequest"/>
    <title value="CH ORF ServiceRequest"/>
    <status value="draft"/>
    <date value="2019-02-05"/>
    <publisher value="eHealth Suisse"/>
    <contact>
        <name value="eHealth Suisse"/>
        <telecom>
            <system value="url"/>
            <value value="https://www.e-health-suisse.ch/"/>
        </telecom>
    </contact>
    <contact>
        <name value="Oliver Egger"/>
        <telecom>
            <system value="email"/>
            <value value="oliver.egger@ahdis.ch"/>
        </telecom>
    </contact>
    <description value="Profile on ServiceRequest for CH ORF"/>
    <jurisdiction>
        <coding>
            <system value="urn:iso:std:iso:3166"/>
            <code value="CH"/>
        </coding>
    </jurisdiction>
    <copyright value="CC-BY-SA-4.0"/>
    <fhirVersion value="4.0.1"/>
    <kind value="resource"/>
    <abstract value="false"/>
    <type value="ServiceRequest"/>
    <baseDefinition value="http://hl7.org/fhir/StructureDefinition/ServiceRequest"/>
    <derivation value="constraint" />
    <differential>
        <element id="ServiceRequest">
            <path value="ServiceRequest"/>
        </element>
        <element id="ServiceRequest.identifier">
            <path value="ServiceRequest.identifier"/>
            <slicing>
                <discriminator>
                    <type value="pattern" />
                    <path value="$this" />
                </discriminator>
                <rules value="open" />
            </slicing>
        </element>
        <element id="ServiceRequest.identifier:placerOrderId">
            <path value="ServiceRequest.identifier" />
            <sliceName value="placerOrderId" />
            <short value="Placer Order Id"/>
            <patternIdentifier>
                <type>
                    <coding>
                        <system value="http://terminology.hl7.org/CodeSystem/v2-0203" />
                        <code value="PLAC" />
                    </coding>
                </type>
            </patternIdentifier>
        </element>
        <element id="ServiceRequest.identifier:placerOrderId.system">
            <path value="ServiceRequest.identifier.system" />
        </element>
        <element id="ServiceRequest.identifier:placerOrderId.value">
            <path value="ServiceRequest.identifier.value" />
            <min value="1" />
        </element>
        <element id="ServiceRequest.identifier:fillerOrderId">
            <path value="ServiceRequest.identifier" />
            <sliceName value="fillerOrderId" />
            <short value="Filler Order Id"/>
            <patternIdentifier>
                <type>
                    <coding>
                        <system value="http://terminology.hl7.org/CodeSystem/v2-0203" />
                        <code value="FILL" />
                    </coding>
                </type>
            </patternIdentifier>
        </element>
        <element id="ServiceRequest.identifier:fillerOrderId.system">
            <path value="ServiceRequest.identifier.system" />
        </element>
        <element id="ServiceRequest.identifier:fillerOrderId.value">
            <path value="ServiceRequest.identifier.value" />
            <min value="1" />
        </element>
        <element id="ServiceRequest.subject">
            <path value="ServiceRequest.subject"/>
            <short value="Patient"/>
            <definition value="The principle target of a particular form content is one patient (for obstetrical and neonatal use cases see...)."/>
            <type>
                <code value="Reference"/>
                <targetProfile value="http://fhir.ch/ig/ch-core/StructureDefinition/ch-core-patient"/>
            </type>
        </element>
        <element id="ServiceRequest.requester">
            <path value="ServiceRequest.requester"/>
            <short value="The person/organization responsible for the form content"/>
            <type>
                <code value="Reference"/>
                <targetProfile value="http://fhir.ch/ig/ch-core/StructureDefinition/ch-core-practitionerrole"/>
            </type>
        </element>
        <element id="ServiceRequest.performer">
            <path value="ServiceRequest.performer"/>
            <short value="Person/organization who receives the request"/>
            <type>
                <code value="Reference"/>
                <targetProfile value="http://fhir.ch/ig/ch-core/StructureDefinition/ch-core-practitionerrole"/>
            </type>
        </element>
    </differential>
</StructureDefinition>